<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.sheedah">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/sheedah_logo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/sheedah_logo_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Sheedah">
        <activity
            android:name=".NewInvoicePage"
            android:exported="true" />
        <activity
            android:name=".RegistrationPage"
            android:exported="true" />
        <activity
            android:name=".HomePageActivity"
            android:exported="true"
            android:theme="@style/Home_page_theme" />
        <activity
            android:name=".LoginPage"
            android:exported="true" />
        <activity
            android:name=".SplashScreen"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>